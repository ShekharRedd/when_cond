CODE_CHANGES=getGitChanges()

pipeline {
  agent any
  environment{
      BRANCH_NAME="main"
      }
    stages{
            stage("build")
                {
                  when{
                    expression{
                      env.BRANCH_NAME=='main' 
                    }
                  }
                  steps 
                    { 
                       echo "hello world"
                    }
                }
            stage("deplov")
                { 
                  steps
                     { 
                     
                     echo "asg"
                     
                     
                     
                     echo "deploy successfully"
                     }
                }
    }
}
