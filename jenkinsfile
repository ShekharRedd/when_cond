pipeline {
  agent any
    stages{
            stage("build")
                {
                  when{
                    expression{
                      env.BRANCH_NAME=='null' || env.CODE_CHANGE=true

                    }
                  }
                  steps 
                    { 
                       echo "hello world"
                    }
                }
            stage("deplov")
                { 
                  steps
                     { 
                     
                     sh ' git branch '
                     
                     
                     echo "deploy successfully"
                     }
                }
    }
}
